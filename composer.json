{
    "name": "olps/payum-component",
    "type": "library",
    "description": "Laminas component for Payum",
    "keywords": [
        "laminas",
        "component",
        "payum"
    ],
    "homepage": "https://github.com/Online-Postal-Scanning/payum-component",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.3",
        "payum/core": "^1.7.5",
        "psr/container": "^1.0 || ^2.0"
    },
    "require-dev": {
        "php-http/guzzle7-adapter": "^1.0",
        "phpunit/phpunit": "^11",
        "payum/offline": "^1.3.7",
        "symfony/dotenv": " ^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "OLPS\\PayumComponent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
      "allow-plugins": {
        "php-http/discovery": true
      }
    }
}